    "baileys": "^7.0.0-rc.9",
    "cache-manager": "^6.4.3",
    "cache-manager-redis-store": "^3.0.1",
    "cacheable": "^1.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
